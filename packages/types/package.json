{
  "name": "@ancore/types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for Ancore",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --tsconfig tsconfig.json",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest --coverage",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ancore",
    "types",
    "typescript"
  ],
  "author": "Ancore Contributors",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.0",
    "@types/jest": "^29.5.0"
  },
  "dependencies": {
    "zod": "^3.22.0",
    "@stellar/stellar-sdk": "^12.0.0"
  }
}